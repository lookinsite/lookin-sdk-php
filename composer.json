{
    "name": "tomohiroukawa/lookin-sdk-php",
    "description": "Lookin client SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomohiro Ukawa",
            "homepage": "https://github.com/tomohiroukawa"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lookin\\": "src/"
        }
    },
    "config": {
        "preffered-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
        "php": ">= 5.6",
        "guzzlehttp/guzzle": "^6.3",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
